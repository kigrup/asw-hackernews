<tr class="athing comtr" id="<%= comment.id %> ">
    <td>
        <table border="0">
            <tbody>
                <tr>
                    <td class="ind" indent="<%=(comment.deep - post.deep - 1)%>">
                        <img src="s.gif" height="1" width="<%=20*(comment.deep - post.deep - 1)%>" />
                    </td>
                    <td valign="top" class="votelinks">
                        <center>
                            <a
                                <% if (comment.isLiked != undefined && comment.isLiked) { %>
                                    class="nosee"
                                <% } %>
                                id="up_<%=post.id%>"
                                href="https://asw.raulplesa.online/vote?id=<%=comment.id%>&amp;how=up">

                                <div class="votearrow" title="upvote"></div>
                            </a>
                        </center>
                    </td>
                    <td class="default">
                        <div style="margin-top: 2px; margin-bottom: -10px;">
                            <span class="comhead">
                                <a href="https://asw.raulplesa.online/user?id=<%= comment.author %>" class="hnuser">
                                    <font color="#3c963c">
                                        <%= comment.authorName %>
                                    </font>
                                </a>
                                <span class="age" title="<%= comment.createdAt %>">
                                    <a href="https://asw.raulplesa.online/item?id=<%= comment.id %>">
                                        <%= moment(new Date(comment.createdAt)).fromNow() %>
                                    </a>
                                </span>
                                <% if (comment.isLiked != undefined && comment.isLiked) { %>
                                    <span class="unvote" title="unvote">
                                        &nbsp;|&nbsp;<a href="https://asw.raulplesa.online/vote?id=<%=comment.id%>&amp;how=un">
                                            unvote
                                        </a>
                                    </span>
                                <% } %> 
                            </span>
                        </div>
                        <br />
                        <div class="comment">
                            <span class="commtext c00">
                                <%= comment.content %>
                            </span>
                            <div class="reply">
                                <p>
                                    <font size="1">
                                        <u><a href="https://asw.raulplesa.online/item?id=<%= comment.id %>">reply</a></u>
                                    </font>
                                </p>
                            </div>
                        </div>
                    </td>
                </tr>
            </tbody>
        </table>
    </td>
</tr>
<% if (comment.contributions) for( let reply=0; reply < comment.contributions.length; reply++ ) { %>
    <%- include('../partials/comment.ejs', {comment: comment.contributions[reply].dataValues}); %> 
<% } %>