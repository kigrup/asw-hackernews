<tr class="athing" id="<%=post.id%>">
    <td align="right" valign="top" class="title">
        <span class="rank">
            <% if (post.postNumber!=undefined) { %>
                <%=post.postNumber%>
            <% } %>
        </span>
    </td>
    <td valign="top" class="votelinks">
        <center>
            <a
                <% if (loggedIn && post.dataValues.isLiked != undefined && post.dataValues.isLiked) { %>
                    class="nosee"
                <% } %>
                <% console.log(`log from ejs: loggedIn=${loggedIn} post.isLiked=${post.isLiked} post.dataValues.isLiked=${post.dataValues.isLiked}`) %> 
                id="up_<%=post.id%>"
                href="http://hackers.hopto.org:13001/vote?id=<%=post.id%>&amp;how=up">

                <div class="votearrow" title="upvote"></div>
            </a>
        </center>
    </td>
    <td class="subtext">
        <span class="score" id="score_<%=post.id%>"><%= post.upvotes %> points</span>
        by
        <a href="http://hackers.hopto.org:13001/user?id=<%= post.author %> " class="hnuser">
            <%= post.authorName %>
        </a>
        <span class="age" title="<%= post.createdAt %> ">
            <a href="http://hackers.hopto.org:13001/item?id=<%=post.id%>">
                <%= moment(new Date(post.createdAt)).fromNow() %> 
            </a>
        </span> |
        <% if (loggedIn && post.dataValues.isLiked != undefined && post.dataValues.isLiked) { %>
            <span class="unvote" title="unvote">
                <a href="http://hackers.hopto.org:13001/vote?id=<%=post.id%>&amp;how=un">
                    unvote
                </a>
            </span> | 
        <% } %> 
        <% if (post.dataValues.inReplyTo != rootPost.dataValues.id) { %>
            <span>
                <a href="http://hackers.hopto.org:13001/item?id=<%=post.dataValues.inReplyTo%>">
                    parent
                </a>
            </span> |
        <% } %>
        <span>
            on:
            <a href="http://hackers.hopto.org:13001/item?id=<%=rootPost.dataValues.id%>">
                <%= rootPost.dataValues.title %> 
            </a>
        </span>
        <% if (post.dataValues.comments > 0) { %>
            |
            <a href="http://hackers.hopto.org:13001/item?id=<%=post.dataValues.id%>">
                <%= post.comments %>&nbsp;replies
            </a>
        <% } else { %>
            |
            <a href="http://hackers.hopto.org:13001/item?id=<%=post.dataValues.id%>">
                reply
            </a>
        <% } %> 
    </td>
</tr>
<tr class="spacer" style="height: 5px"></tr>
