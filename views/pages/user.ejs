<html lang="en" op="user">
    <head>
        <title>Profile: <%= user.username %>  | Hacker News</title>
        <%- include('../partials/head'); %>
    </head>
    <body cz-shortcut-listen="true">
        <center>
            <table
                id="hnmain"
                border="0"
                cellpadding="0"
                cellspacing="0"
                width="85%"
                bgcolor="#f6f6ef"
            >
                <tbody>
                    <tr>
                        <td bgcolor="#ff6600">
                            <table
                                border="0"
                                cellpadding="0"
                                cellspacing="0"
                                width="100%"
                                style="padding: 2px"
                            >
                                <tbody>
                                    <%- include('../partials/indexheader.ejs', { loggedIn: loggedIn, user: user }); %>
                                </tbody>
                            </table>
                        </td>
                    </tr>
                    <tr
                        id="pagespace"
                        title="Profile: RageoftheRobots"
                        style="height: 10px"
                    ></tr>
                    <tr>
                        <td>
                            <table border="0">
                                <tbody>
                                    <tr class="athing">
                                        <td valign="top">user:</td>
                                        <td timestamp="1631965655">
                                            <a
                                                href="https://news.ycombinator.com/user?id=RageoftheRobots"
                                                class="hnuser"
                                                ><%= user.username %> </a
                                            >
                                        </td>
                                    </tr>
                                    <tr>
                                        <td valign="top">created:</td>
                                        <td>
                                            <a
                                                href="https://news.ycombinator.com/front?day=2021-09-18&amp;birth=RageoftheRobots"
                                                >42 days ago</a
                                            >
                                        </td>
                                    </tr>
                                    <tr>
                                        <td valign="top">karma:</td>
                                        <td><%= user.karma %> </td>
                                    </tr>
                                    <tr>
                                        <td valign="top">about:</td>
                                        <td style="overflow: hidden"><%= user.about %> </td>
                                    </tr>
                                    <tr>
                                        <td></td>
                                        <td>
                                            <a
                                                href="https://news.ycombinator.com/submitted?id=<%= user.id %> "
                                                ><u>submissions</u></a
                                            >
                                        </td>
                                    </tr>
                                    <tr>
                                        <td></td>
                                        <td>
                                            <a
                                                href="https://news.ycombinator.com/threads?id=<%= user.id %> "
                                                ><u>comments</u></a
                                            >
                                        </td>
                                    </tr>
                                    <tr>
                                        <td></td>
                                        <td>
                                            <a
                                                href="https://news.ycombinator.com/favorites?id=<%= user.id %> "
                                                ><u>favorites</u></a
                                            >
                                        </td>
                                    </tr>
                                </tbody>
                            </table>
                            <br /><br />
                        </td>
                    </tr>
                </tbody>
            </table>
        </center>
        <script type="text/javascript" src="./data/hn.js"></script>
    </body>
</html>
